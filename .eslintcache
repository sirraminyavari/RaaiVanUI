ate && prevShowState)\n    GlobalUtilities.after_fade_out(() => {\n      _setDisposed(true);\n    });\n\n  return disposed || (!showState && !prevShowState) ? (\n    <></>\n  ) : (\n    <Container\n      id={componentId}\n      className={`RevDirection ${showState ? 'rv-fade-in' : 'rv-fade-out'}`}\n      noBackground={noBackground}\n      middle={middle}\n      onClick={(e) => {\n        e.stopPropagation();\n        setShowState(false);\n      }}>\n      <ContentContainer>\n        <ContentSection\n          className={`Direction rv-border-radius-half SurroundingShadow ${\n            contentClass || ' '\n          }`}\n          onClick={(e) => e.stopPropagation()}\n          contentWidth={contentWidth}>\n          {!title && stick ? (\n            <></>\n          ) : (\n            <>\n              <TitleContainer className=\"rv-border-radius-half rv-ignore-bottom-radius\">\n                {!stick && <EmptyTitleSide />}\n                <TitleArea\n                  className={`${titleClass ? titleClass : 'WarmColor'}`}>\n                  {title}\n                </TitleArea>\n                {!stick && (\n                  <ExitButton\n                    className=\"rv-circle RevTextAlign\"\n                    onClick={(e) => {\n                      e.stopPropagation();\n                      setShowState(false);\n                    }}>\n                    <CloseIcon />\n                  </ExitButton>\n                )}\n              </TitleContainer>\n            </>\n          )}\n          <MainContent>{props.children}</MainContent>\n        </ContentSection>\n      </ContentContainer>\n    </Container>\n  );\n};\n\nexport default Modal;\n\nconst Container = styled.div`\n  display: flex;\n  flex-flow: column;\n  position: fixed;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  margin: auto;\n  width: 100%;\n  overflow: auto;\n  z-index: ${GlobalUtilities.zindex.dialog()};\n  ${({ middle }) => middle && `justify-content: center; padding-bottom: